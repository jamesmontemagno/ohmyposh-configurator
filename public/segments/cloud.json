      {
        "name": "fetch_version",
        "type": "boolean",
        "default": true,
        "description": "Fetch Azure Functions CLI version"
      },
      {
        "name": "cache_duration",
        "type": "string",
        "default": "none",
        "description": "Cache duration (e.g., \"5m\", \"1h\")"
      },
      {
        "name": "missing_command_text",
        "type": "string",
        "default": "",
        "description": "Text when command is missing"
      }
