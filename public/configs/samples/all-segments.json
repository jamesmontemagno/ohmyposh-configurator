  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }} ",
          "foreground": "p:text-light",
          "background": "p:os-bg"
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue795 {{ .Name }} ",
          "foreground": "p:text-light",
          "background": "p:shell-bg"
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf007 {{ .UserName }}@{{ .HostName }} ",
          "foreground": "p:text-light",
          "background": "p:session-bg"
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0e7 ",
          "foreground": "p:text-light",
          "background": "p:root-bg"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf07c {{ .Path }} ",
          "foreground": "p:text-light",
          "background": "p:path-bg"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue725 {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:text-light",
          "background": "p:git-bg"
        },
        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{ .Code }}{{ end }} ",
          "foreground": "p:text-light",
          "background": "p:status-bg"
        }
      ]
    },
    {
